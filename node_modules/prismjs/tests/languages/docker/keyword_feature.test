ONBUILD ADD . /app/src
FROM ubuntu
MAINTAINER SvenDowideit@home.org.au
RUN cd /tmp
EXPOSE 5900
ENV myName John Doe
COPY hom* /mydir/
VOLUME /myvol
USER daemon
WORKDIR /a
CMD echo "foo"
LABEL version="1.0"
ENTRYPOINT ["top", "-b"]

