`foo bar`
`foo
bar`
`40+2=${40+2}`
`${foo()}`

----------------------------------------------------

[
	["template-string", [
		["string", "`foo bar`"]
	]],
	["template-string", [
		["string", "`foo\r\nbar`"]
	]],
	["template-string", [
		["string", "`40+2="],
		["interpolation", [
			["interpolation-punctuation", "${"],
			["number", "40"],
			["operator", "+"],
			["number", "2"],
			["interpolation-punctuation", "}"]
		]],
		["string", "`"]
	]],
	["template-string", [
        ["string", "`"],
        ["interpolation", [
            ["interpolation-punctuation", "${"],
            ["function", "foo"],
            ["punctuation", "("],
            ["punctuation", ")"],
            ["interpolation-punctuation", "}"]
        ]],
        ["string", "`"]
    ]]
]

