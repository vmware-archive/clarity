{
    "name": "clr-all",
    "version": "0.11.14",
    "private": true,
    "scripts": {
        "start": "webpack-dev-server -d --open",
        "clean": "del ./dist",
        "cpy": "cpy",
        "build": "npm-run-all clean build:angular webpack build:icons",
        "postbuild": "node ./scripts/aot-check.js",
        "pretest": "webpack",
        "test": "karma start ./karma.conf.js",
        "test:travis": "npm-run-all clang:check tslint:check tslint:spec test",
        "clang:check": "./scripts/clang-check.sh",
        "clang:format": "node_modules/.bin/clang-format -style=file -i --glob=src/**/*.ts",
        "prebuild:angular": "cpy ./npm/clr-angular/package.json ./npm/clr-angular/README.md ./src/clr-angular/; replace '@VERSION' $npm_package_version ./src/clr-angular/package.json",
        "build:angular": "ng-packagr -p ./src/clr-angular/ng-package.json",
        "postbuild:angular": "webpack --config webpack.dark-theme.config.js; cpy dist/clr-angular/bundles/* dist/clr-angular; npm run schematics; cd dist/clr-angular; npm pack;",
        "build:icons": "webpack --config webpack.icons.config.js",
        "postbuild:icons": "node ./scripts/clr-icons-svg.js",
        "publish:all": "node scripts/publish.js",
        "cssTest": "./scripts/docker-cdc.js -t",
        "cssUpdate": "./scripts/docker-cdc.js -u",
        "replace": "replace",
        "schematics:test": "cd packages/schematics; npm test; cd ../../;",
        "schematics": "cd packages/schematics; npm run build; cd ../../; npm run schematics:copy",
        "schematics:copy": "cpy '**/*.*' ../../../dist/clr-angular/schematics --parents --cwd=packages/schematics/src",
        "tslint:check": "node_modules/.bin/tslint -p . -c tslint.json \"src/**/*.ts\" -e \"**/node_modules/**\" -e \"src/ks-app/**\"",
        "tslint:spec": "node_modules/.bin/tslint -p . -c tslint.spec.json \"src/**/*.spec.ts\" -e \"**/node_modules/**\" -e \"src/ks-app/**\"",
        "tslint:fix": "node_modules/.bin/tslint -p . -c tslint.json --fix \"src/**/*.ts\" -e \"**/node_modules/**\" -e \"src/ks-app/**\"",
        "webpack": "webpack --progress --colors",
        "preks:publish": "npm run build",
        "ks:publish": "cd src/ks-app; npm run build; cd ../../dist/ks-app; cat index.html > 404.html; surge; cd ../../",
        "dev:publish": "npm run build; cat ./dist/index.html > ./dist/200.html; surge --domain clarity-forms.surge.sh --project ./dist;"
    },
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/router": "^5.0.0",
        "@webcomponents/custom-elements": "^1.0.0",
        "bootstrap": "4.0.0-alpha.5",
        "core-js": "^2.4.1",
        "font-awesome": "^4.5.0",
        "prismjs": "^1.6.0",
        "rxjs": "^5.5.0",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular-devkit/core": "^0.4.7",
        "@angular-devkit/schematics": "^0.4.7",
        "@angular/cli": "1.6.0",
        "@types/commander": "^2.9.1",
        "@types/jasmine": "~2.5.53",
        "@types/jasmine-matchers": "^0.2.30",
        "@types/node": "~6.0.60",
        "@types/selenium-webdriver": "^2.53.33",
        "angular2-template-loader": "^0.6.2",
        "autoprefixer": "^6.5.3",
        "awesome-typescript-loader": "^3.2.3",
        "cheerio": "^0.22.0",
        "circular-dependency-plugin": "^3.0.0",
        "clang-format": "1.2.2",
        "colors": "^1.1.2",
        "commander": "^2.11.0",
        "copy-webpack-plugin": "^4.0.1",
        "cpy-cli": "^1.0.1",
        "css-loader": "^0.28.1",
        "del-cli": "^1.1.0",
        "detect-browser": "^2.0.0",
        "exports-loader": "^0.6.3",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.10.0",
        "html-loader": "^0.5.1",
        "html-webpack-plugin": "^2.29.0",
        "ip": "^1.1.5",
        "istanbul-instrumenter-loader": "^2.0.0",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-edge-launcher": "^0.4.2",
        "karma-firefox-launcher": "^1.1.0",
        "karma-htmlfile-reporter": "^0.3.5",
        "karma-ie-launcher": "^1.0.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-jasmine-matchers": "^3.7.0",
        "karma-mocha-reporter": "^2.2.4",
        "karma-notify-reporter": "^1.0.1",
        "karma-safari-launcher": "^1.0.0",
        "karma-sauce-launcher": "^1.2.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.4",
        "lite-server": "^2.3.0",
        "loader-utils": "^1.1.0",
        "ng-packagr": "^2.0.0",
        "npm": "^4.6.1",
        "npm-run-all": "^4.1.1",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "postcss-loader": "^1.3.3",
        "postcss-url": "^5.1.2",
        "prompt": "^1.0.0",
        "raw-loader": "^0.5.1",
        "replace": "^0.3.0",
        "sass-loader": "^6.0.3",
        "shelljs": "^0.7.8",
        "source-map-loader": "^0.2.0",
        "style-loader": "^0.13.1",
        "suppress-chunks-webpack-plugin": "0.0.4",
        "text-transform-loader": "^2.0.0",
        "ts-node": "~3.2.0",
        "tsickle": "^0.25.6",
        "tslint": "~5.7.0",
        "tslint-defocus": "^2.0.3",
        "typescript": "~2.4.2",
        "url-loader": "^0.5.7",
        "webpack": "~3.6.0",
        "webpack-concat-plugin": "1.4.0",
        "webpack-dev-server": "~2.7.1"
    }
}
